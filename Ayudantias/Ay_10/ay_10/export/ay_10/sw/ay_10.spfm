<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="ay_10"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
ay_10
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="ay_10">
        <sdx:configuration sdx:name="ay_10"
                           sdx:displayName="ay_10"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>ay_10</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="ay_10/boot/ay_10.bif"
                            sdx:readme="ay_10/boot/generic.readme"
                            sdx:qemuArguments="ay_10/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="ay_10/boot/ay_10.bif"
                            sdx:readme="ay_10/boot/generic.readme"
                            sdx:qemuArguments="ay_10/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="ay_10/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="ay_10/standalone_domain/bspinclude/include"
                        sdx:bspConfig="ay_10/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
